
#Commande pour cloner un répository
git clone "Adresse HTTPS ou clé SSH"

#Les 3 commandes de push
git push -all
	 -force
	 -tasks

#Commande pour pull
git pull "Adresse HTTPS ou clé SSH"

#Ajout d'une branche
git branch {nom de branche}

#Changer de branch
git checkout {nom de la branche où aller}
